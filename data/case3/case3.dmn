  <decision id="decision_uptodate" name="uptodate(ResourceCollection src, ResourceCollection target)">
    <variable name="uptodate(ResourceCollection src, ResourceCollection target)" typeRef="boolean" />
    <informationRequirement>
      <requiredDecision href="#decision_missingSources" />
      <requiredInput href="#input_newestSourceModified" />
    </informationRequirement>
    <informationRequirement id="InformationRequirement_0x9x39c">
