    <decisionTable>
      <input id="input3" label="neTargets">
        <inputExpression typeRef="boolean">
          <text>missingTargets</text>
        </inputExpression>
      </input>
      <input id="input4" label="neSources">
        <inputExpression typeRef="boolean">
          <text>missingSources</text>
        </inputExpression>
      </input>
      <output id="output3" label="uptodate(ResourceCollection src, ResourceCollection target)" name="uptodate" typeRef="boolean" />
      <rule>
        <inputEntry>
          <text>neTargets &gt; 0</text>
        </inputEntry>
        <inputEntry>
          <text>-</text>
        </inputEntry>
        <outputEntry>
          <text>return false;</text>
        </outputEntry>
      </rule>
      <rule>
        <inputEntry>
          <text>neTargets &lt;= 0</text>
        </inputEntry>
        <inputEntry>
          <text>neSources &gt; 0</text>
        </inputEntry>
        <outputEntry>
          <text>return false;</text>
        </outputEntry>
      </rule>
      <rule>
        <inputEntry>
          <text>neTargets &lt;= 0</text>
        </inputEntry>
        <inputEntry>
          <text>neSources &lt;= 0</text>
        </inputEntry>
        <outputEntry>
          <text>return oldestTarget.getLastModified() &gt;= newestSource.getLastModified();</text>
        </outputEntry>
      </rule>
    </decisionTable>
  </decision>
